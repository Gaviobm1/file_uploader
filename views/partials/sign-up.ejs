<label for="first_name">First Name:</label>
<input type="text" name="first_name" id="first_name" value = "<% typeof user !==
'undefined' ? user.firstName : '' %>""/>
<label for="last_name">Last Name:</label>
<input type="text" name="last_name" id="last_name" value = "<% typeof user !==
'undefined' ? user.lastName : '' %>""/>
<label for="email">Email:</label>
<input type="email" name="email" id="email" value = "<% typeof user !==
'undefined' ? user.email : '' %>""/>
<label for="password">Password:</label>
<input type="password" name="password" id="password" />
<label for="password_confirm">Confirm Password:</label>
<input type="password" name="password_confirm" id="password_confirm" />
<button type="submit">Sign Up</button>
